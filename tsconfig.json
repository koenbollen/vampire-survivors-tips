{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2020",
    "module": "es2020",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true
  }
}
